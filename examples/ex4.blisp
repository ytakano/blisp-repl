(data (Maybe t)
    (Just t)
    Nothing)

(export head (x) (Pure (-> ('(Int)) (Maybe Int)))
    (match x
        ((Cons n _) (Just n))
        (_ Nothing)))

(export tail (x) (Pure (-> ('(Int)) (Maybe Int)))
    (match x
        (Nil Nothing)
        ((Cons n Nil) (Just n))
        ((Cons _ l) (tail l))))
